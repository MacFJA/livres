{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "api-platform/api-pack": "^1.2",
        "doctrine/annotations": "^1.10",
        "easycorp/easyadmin-bundle": "^2.3",
        "enqueue/dsn": "^0.10.1",
        "fale/isbn": "^3.1",
        "fire015/flintstone": "^2.2",
        "gabrielelana/byte-units": "^0.5.0",
        "guzzlehttp/guzzle": "^6.5",
        "imagine/imagine": "^1.2",
        "javiereguiluz/easyslugger": "^1.0",
        "liip/imagine-bundle": "^2.3",
        "luchaninov/csv-file-loader": "^1.7",
        "macfja/book-retriever": "^1.0.0",
        "macfja/redisearch-integration": "^1.0",
        "nyholm/psr7": "^1.3",
        "pcrov/jsonreader": "^1.0",
        "php-http/curl-client": "^2.1",
        "predis/predis": "^1.1",
        "psr/log": "^1.1",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "*",
        "symfony/twig-pack": "*",
        "symfony/validator": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "drenso/phan-extensions": "^3.0",
        "enlightn/security-checker": "^1.1",
        "ergebnis/composer-normalize": "^2.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "insolita/unused-scanner": "^2.1",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phan/phan": "^3.0",
        "phploc/phploc": "^6.0",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12.25",
        "rskuipers/php-assumptions": "^0.8.0",
        "sebastian/phpcpd": "^6.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/polyfill-mbstring": "< 1.22.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "vimeo/psalm": "^3.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
